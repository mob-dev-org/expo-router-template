{
  "name": "@toyamarodrigo/expo-router-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "web": "expo start --web",
    "prepare": "husky install",
    "-----------------------------": "------------------------------",
    "tscheck": "tsc -p . --noEmit",
    "create:devClient": "eas build --profile development",
    "create:devClient:simulator": "eas build --profile development-simulator",
    "ios:--------------": "------------------------------",
    "ios:release:device": "yarn ios --device --configuration Release",
    "ios": "expo run:ios --device",
    "ios:releasePreviewToExpo": "eas build -p ios --profile preview --no-wait --auto-submit",
    "ios:build-preview-locally": "eas build --profile preview --platform ios --no-wait --local",
    "ios:build-production-locally": "eas build --profile production --platform ios --no-wait --local",
    "ios:releaseProductionToExpo": "eas build --platform ios --profile production --no-wait --auto-submit",
    "ios:installReleaseToDevice": "npx expo run:ios --device --configuration Release",
    "android:--------------": "------------------------------",
    "android": "expo run:android --device",
    "android:installToDevice": "yarn reverse && npx expo run:android --device",
    "android:releasePreviewToExpo": "eas build -p android --profile preview --no-wait --auto-submit",
    "android:build:development": "eas build --profile development --platform android",
    "android:build:preview": "eas build --profile preview --platform android",
    "android:build:preview-locally": "eas build --profile preview --platform android --no-wait --local"
  },
  "dependencies": {
    "@lukemorales/query-key-factory": "^1.3.2",
    "@react-native-community/netinfo": "11.1.1",
    "@react-navigation/drawer": "^6.6.6",
    "@shopify/flash-list": "1.6.3",
    "@tanstack/react-query": "^5.12.1",
    "axios": "^1.6.2",
    "expo": "^49.0.21",
    "expo-constants": "^14.4.2",
    "expo-linking": "~6.0.0",
    "expo-router": "^2.0.14",
    "expo-splash-screen": "~0.22.0",
    "expo-status-bar": "~1.7.1",
    "react": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-native": "^0.72.7",
    "react-native-gesture-handler": "~2.12.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.7.4",
    "react-native-screens": "~3.27.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@tanstack/eslint-plugin-query": "^5.0.5",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/react": "^18.2.40",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "metro": "^0.79.1",
    "metro-resolver": "^0.79.1",
    "prettier": "^3.1.0",
    "tailwindcss": "3.3.5"
  },
  "resolutions": {
    "metro": "^0.79.1",
    "metro-resolver": "^0.79.1"
  },
  "overrides": {
    "metro": "^0.79.1",
    "metro-resolver": "^0.79.1"
  }
}
